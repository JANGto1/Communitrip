    axios.post('http://localhost:8080/users/login',{\n        email: loginInfo.email,\n        password: loginInfo.password\n      }).then( handleResponseSuccess )\n      navigate('/')\n      openModalHandler();\n    }else{\n      setErrorMessage('이메일과 비밀번호를 입력하세요')\n    }\n  };\n\n  return (\n    <div>\n\n          {isOpen ? <ModalBackdrop onClick={openModalHandler}>\n                    <ModalView onClick={(event) => {event.stopPropagation()}}>  \n        <center>\n        <h1>로그인 하기</h1>\n        <br /><br />\n        <form onSubmit={(e) => e.preventDefault()}>\n          <div>\n            <div>이메일 입력</div>\n            <Input type='email' onChange={handleInputValue('email')} />\n          </div>\n          <br />\n          <div>\n            <div>비밀번호 입력</div>\n            <Input\n              type='password'\n              onChange={handleInputValue('password')}\n            />\n          </div>\n          <br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br />\n          <ModalBtn  onClick={handleLogin}>\n            로그인\n          </ModalBtn>\n          <br /><br />\n          <div>\n          <ModalBtn  className=\"close-btn\" onClick={setSignUpModalOn} >\n            회원가입\n          </ModalBtn>\n          </div>\n          <div className='alert-box'>{errorMessage}</div>\n\n          <Signup show={signUpModalOn} onHide={() => setSignUpModalOn(false)} />\n\n        </form>\n      </center>         \n                    </ModalView>\n                  </ModalBackdrop> : null} \n\n    </div>\n  );\n};\n",[],"/Users/jangtaeseong/Desktop/second-project/Communitrip/client/src/pages/Main.js",["72","73"],[],"/Users/jangtaeseong/Desktop/second-project/Communitrip/client/src/pages/Mypage.js",[],[],"/Users/jangtaeseong/Desktop/second-project/Communitrip/client/src/pages/Signup/Validation.js",["74","75"],[],"// 유효성 검사 : 이메일 작성시 (. , @)는 반드시 한개만 입력 가능\nexport function checkUserEmail(email) {\n  return /^[0-9a-zA-Z]([-_\\.]?[0-9a-zA-Z])*@[0-9a-zA